/dts-v1/;
/plugin/;

/ {
	compatible = "krtkl,snickerdoodle", "xlnx,zynq-7000";

	fragment@0 {
		target = <&amba>;
		__overlay__ {

			#address-cells = <1>;
			#size-cells = <1>;
			
			uio0: uio0@40000000 {
				reg = <0x40000000 0x80000000>;
				compatible = "krtkl,generic-uio,ui_pdrv";
				status = "okay";
				interrupt-parent = <&intc>;
				interrupts = <0 29 4>;
			};
		};
	};

	fragment@1 {
		target-path = "\__symbols__";
		__overlay__ {
			uio0 = &uio0;
		};
	};
};


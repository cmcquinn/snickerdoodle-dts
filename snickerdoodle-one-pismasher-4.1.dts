/dts-v1/;
/include/ "snickerdoodle-one.dtsi"

/ {
	aliases {
		ethernet0 = &gem0;
		ethernet1 = &gem1;
	};

	power_en_reg: fixed-regulator-powen {
		compatible = "regulator-fixed";
		regulator-name = "wlan-en-regulator";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		gpio = <&gpio0 20 4>;
		startup-delay-us = <90000>;
		enable-active-high;
	};

	usb_phy0: usb-phy0 {
		compatible = "usb-nop-xceiv";
		#phy-cells = <0>;
		reset-gpios = <&gpio0 16 1>;
		view-port = <0x170>;
		drv-vbus;
	};
};

&i2c1 {
	status = "okay";
	clock-frequency = <100000>;
	
	usb2514b@2c {
		compatible = "microchip,usb2514b";
		reg = <0x2c>;
	};
};

&usb0 {
	status = "okay";
	dr_mode = "host";
	usb-phy = <&usb_phy0>;
};

&gem0 {
	status = "okay";
	phy-mode = "rgmii-id";

	eth_phy0: ethernet-phy@0 {
		reg = <0>;
		device_type = "ethernet-phy";
		reset-gpios = <&gpio0 19 1>;
	};

	gmii2rgmii0: gmiitorgmii@8 {
		compatible = "xlnx,gmii-to-rgmii-1.0";
		reg = <8>;
		phy-handle = <&eth_phy0>;
	};
};

&gem1 {
	status = "okay";
	phy-mode = "rgmii-id";

	eth_phy1: ethernet-phy@1 {
		reg = <1>;
		device_type = "ethernet-phy";
		reset-gpios = <&gpio0 17 1>;
	};

	gmii2rgmii1: gmiitorgmii@9 {
		compatible = "xlnx,gmii-to-rgmii-1.0";
		reg = <9>;
		phy-handle = <&eth_phy1>;
	};
};
